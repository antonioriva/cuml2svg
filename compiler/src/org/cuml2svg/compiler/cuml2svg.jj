//ATTRIBUTES → ATTRIBUTE_DEFINITIONS
//ATTRIBUTES → ε
void attributes(Class currentDefinition):{
	ArrayList<Attribute> l;
}{
	{l=new ArrayList<Attribute>();}
	<ATTRIBUTES> "{" (attribute(currentDefinition,l))+ "}"
}



////////////////////////////////////////////////////////////////////////////
//ATTRIBUTE → COMMENT VISIBILITY ATTRIBUTE_TYPE attribute_name DEFAULT_VALUE
void attribute(Class currentDefinition, ArrayList<Attribute> l):{
	Token vis; Attribute a;String s;
}{
	(<COMMENT>)? 
	
	(vis=<VISIBILITY>)? 
	(a=typed_attribute_name() | a=attribute_name() )
	
	(s=default_value(){a.setDefaultValue(s);})? ";"
	{
		if(!isAttributeAlreadyDeclared(l,a)){
			currentDefinition.addAttribute(a);
			l.add(a);
		}else{
			//TODO ottenere il numero di linea e di colonna dal token
			printError("attribute "+a.getAttributeName()+" already declared, this definition will be skipped",-1,-1);
		}
		}
	
}

Attribute attribute_name():{Token t;Attribute a;}{
	t=<VARIABLE>
	{a=new Attribute(t.image);
	return a;}
}

Attribute typed_attribute_name():{Token t,t1;Attribute a;}{
	t=<VARIABLE> t1=<VARIABLE>{
	a = new Attribute(t1.image);
	a.setAttributeType(t.image);
	return a;
	}
}



