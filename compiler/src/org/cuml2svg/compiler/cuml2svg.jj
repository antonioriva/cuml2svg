/////////////////////////////////////////////////////////////////////
//METHOD → OPTIONAL_COMMENT VISIBILITY METHOD_TYPE method_name { METHOD_ARGS }
//METHOD_TYPE → TYPE
void method(Class currentDefinition):{
	Token vis,name; Attribute a;
}{
	(<COMMENT>)? 
	(vis=<VISIBILITY>)? 
	(name=typed_method() | name=method_name())
	{
		Method m=new Method(name.image);
	}
	"(" (a=method_arg(){m.addParameter(a);}
	
	("," a=method_arg(){m.addParameter(a);})* )? ")" ";"
	{
		if(!isMethodAlreadyDeclared(currentDefinition,m)){
		currentDefinition.addMethod(m);
		}else{
			printError("method "+m.getMethodName()+" already declared; this definition will be skipped",-1,-1);
		}
	}
}

Token typed_method():{Token name;}{
	<VARIABLE> name=<VARIABLE> 
	{
		return name;
	}
}

Token method_name():{Token name;}{
	name=<VARIABLE> 
	{
		return name;
	}
}

